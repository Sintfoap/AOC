
des parterum(sses, delimes) {
  reserum:= []
  wcerum := sserum
  sa := 0
  ea := 0
  pro ea in 0.<wca {
    si sses[ea] == delimes {
        reserum.inseri(resa, sses[sa.<ea])
        sa = ea + 1
    }
  }
  redeo reserum
}

